{
  "name": "stimmer-rxjs",
  "description": "Rxjs adapter for stimmer",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Chris Scribner",
  "repository": {
    "type": "git",
    "url": "https://github.com/scriby/stimmer"
  },
  "devDependencies": {
    "@types/jest": "23.3.13",
    "@types/node": "10.12.21",
    "jest": "23.6.0",
    "reselect": "4.0.0",
    "source-map-support": "0.5.10",
    "ts-jest": "23.10.5",
    "typescript": "3.3.1"
  },
  "peerDependencies": {
    "rxjs": " ^6.4.0",
    "stimmer": "^0.0.1"
  },
  "files": [
    "dist/src/**",
    "src/**",
    "tsconfig.json"
  ],
  "main": "dist/src/index.js",
  "scripts": {
    "build": "rm -rf dist/* && tsc",
    "coverage": "jest src --coverage",
    "test": "jest src --watch",
    "test-debug": "node --inspect-brk ./node_modules/.bin/jest src --runInBand --watch"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "((\\.|/)test)\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "stimmer": "<rootDir>/../store/src/index.ts"
    }
  }
}
